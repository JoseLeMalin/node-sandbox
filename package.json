{
  "name": "node-sandbox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "npx eslint src/ --ext .js --ext .ts",
    "format": "pnpm exec prettier src/ --write",
    "build": "tsc -p .",
    "seed": "npx ts-node prisma/seeders/users.ts",
    "dev": "npx @dotenvx/dotenvx run --env-file=.env.dev -- nodemon src/app.ts",
    "devbis&": "npx @dotenvx/dotenvx run --env-file=.env.dev -- node --loader tsx watch src/app.ts",
    "devbis": "pnpx @dotenvx/dotenvx run --env-file=.env.dev -- ts-node src/app.ts",
    "start": "node dist/app.js",
    "test": "jest"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.15.4",
    "@prisma/adapter-pg": "^5.9.1",
    "@prisma/client": "^5.9.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "debug": "~2.6.9",
    "dotenv": "^16.4.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.3",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "prisma": "^5.9.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}

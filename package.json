{
  "name": "node-sandbox",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "npx eslint src/ --ext .js --ext .ts",
    "format": "pnpm exec prettier src/ --write",
    "build": "tsc -p .",
    "docker:build": "docker-compose --file docker/compose.yaml up --build",
    "docker:down": "docker-compose --file docker/compose.yaml down",
    "seed": "npx ts-node ./prisma/seeders/mtg-cards.ts",
    "dev": "npx @dotenvx/dotenvx run --env-file=.env.dev -- nodemon src/app.ts",
    "devbis&": "dotenvx run --env-file=.env.dev -- ts-node src/app.ts",
    "devbis": "ts-node src/app.ts",
    "start": "node dist/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "0.15.4",
    "@faker-js/faker": "^8.4.1",
    "@prisma/adapter-pg": "5.11.0-integration-fix-debug-reexport.2",
    "@prisma/client": "5.11.0-integration-fix-debug-reexport.2",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "debug": "~2.6.9",
    "express": "~4.16.4",
    "express-session": "^1.18.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/express-session": "^1.18.0",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.20",
    "@types/pg": "^8.11.2",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "@babel/core": "^7.23.9",
    "bufferutil": "^4.0.8"
  }
}
